@model IEnumerable<Web_HotelManagement.Models.ModelsView.Booking_View>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Hello World</h2>

<table class="table table-striped">
    <tr>
        <th>FullName</th>
        <th>Gender</th>
        <th>Email</th>
        <th>Phone</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FullName</td>
            <td>@item.Gender</td>
            <td>@item.Email</td>
            <td>@item.Phone</td>
        </tr>
    }
</table>